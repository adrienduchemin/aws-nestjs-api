{
  "name": "aws-nestjs-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "prezip": "rimraf bundle.zip && npm run build && npm run install:prod",
    "preinstall:prod": "rimraf node_modules",
    "install:prod": "npm ci --only=production",
    "postzip": "npm ci",
    "zip": "bestzip bundle.zip dist/src/* node_modules/*",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/platform-express": "9.2.1",
    "aws-lambda": "1.0.6",
    "aws-sdk": "2.668.0",
    "aws-serverless-express": "3.3.8",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.36.1",
    "@aws-cdk/aws-lambda": "1.36.1",
    "@aws-cdk/core": "1.36.1",
    "@aws-cdk/cx-api": "1.36.1",
    "@nestjs/cli": "7.1.4",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@types/aws-lambda": "8.10.51",
    "@types/aws-serverless-express": "3.3.3",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "aws-cdk": "1.36.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.5.3",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
